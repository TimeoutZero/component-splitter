{
	"name": "vue-splitter",
	"displayName": "vue-splitter",
	"description": "Splitter for main tags on vue files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.31.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.truco"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
      "command": "extension.truco",
			"title": "VSTV Vue Split Vertical"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
